generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Barang {
  id        Int      @id @default(autoincrement())
  nama      String
  stok      Int
  harga     Int
  createdAt DateTime @default(now())
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}

